#!/usr/bin/lua

local site = require "gluon.site"

local f = io.open("/tmp/range6","r")
local g = io.open("/tmp/addr6", "r")

if f and g then
    local range6 = f:read('*a')
    local addr6  = g:read('*a')
    f:close()
    g:close()

    io.write("-i br-client -p " .. range6)
    io.write(" --rdnss " .. addr6)
end
